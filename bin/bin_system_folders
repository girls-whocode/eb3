system_folders() {
    echo "${eb3_BinPath}"
    echo "${eb3_CachePath}"
    echo "${eb3_TempPath}"
    echo "${eb3_LibPath}"
    echo "${eb3_ModPath}"
    echo "${eb3_EtcPath}"
    echo "${eb3_ConfPath}"
    echo "${eb3_OptPath}"
    echo "${eb3_UsrPath}"
    echo "${eb3_OverRidesPath}"
    echo "${eb3_ThemesPath}"
    echo "${eb3_VarPath}"
    echo "${eb3_LogsPath}"
}

success "Loaded $(basename "${BASH_SOURCE}")" >> "${eb3_LogsPath}startup.log"